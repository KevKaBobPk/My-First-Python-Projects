usernames = ["John", "Babu", "Sally", "Tessy", "Kevin", "admin"]

for username in usernames:
    if username == "admin":
        print("Hello admin, would you like to see a status report?")
    else:
        print("Hello  " + username)

if usernames:
    print("There is at least one user.")
else:
    print("We need to find some users.")

current_users = ["John", "Babu", "Sally", "Tessy," "Kevin", "admin"]
new_users = ["Eric", "BABU", "Billy", "Tessy," "Kevin", "Jordan"]

for new_user in new_users:
    if new_user.title() in current_users:
        print("Sorry, you will need to enter a different username.")
    else:
        print("This username is available.")
